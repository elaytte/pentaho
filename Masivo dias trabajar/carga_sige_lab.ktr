    <connection>SIGE_LAB</connection>
    <sql>SELECT ANO_ESCOLAR, MES_ESCOLAR,  RBD, COD_ENS, COD_GRA, LET_CUR, -1 origen_dia30 
FROM DB2ADMIN.SIGE_DIAS_TRABAJAR
WHERE ANO_ESCOLAR = 2021 AND MES_ESCOLAR = 6
    AND RBD IN (select RBD
                from DB2ADMIN.SIGE_ESTABLECIMIENTO A
                where   A.estado_estab = 1
                        AND A.TIPO_DEPENDENCIA in (1,2,3,5,6)
                        and A.cod_region not in (11,12)
						
