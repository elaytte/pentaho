    <sql>update sige_asistencia_declarada S,
            (select A.ANO_ESCOLAR,A.RBD,A.COD_ENS,A.COD_GRA,A.LET_CUR
            from sige_estructura_curso A, sige_curso_autorizado_nivel B
            where A.RBD = B.RBD AND
                A.ANO_ESCOLAR = B.ANO_ESCOLAR AND
                A.COD_ENS = B.COD_ENS AND
                A.COD_GRA = B.COD_GRA AND
                A.HABILITADO_SUBV = 1 AND
                A.ESTADO_HABILITACION = 1 AND
                A.APLICA_JECD = 1 AND
                B.CCJECD &gt; 0 AND
                B.FECHA_JECD &lt;&gt; &apos;1900-01-01&apos; AND
                B.FECHA_JECD &lt; concat(&apos;2015-&apos;,${v_mes_actual}+1,&apos;-01&apos;)
                ) T
set S.JECD = 1
where S.ANO_ESCOLAR = T.ANO_ESCOLAR AND
      S.RBD = T.RBD AND
      S.COD_ENS = T.COD_ENS AND
      S.COD_GRA = T.COD_GRA AND
      S.LET_CUR = T.LET_CUR AND
      S.ANO_ESCOLAR = ${v_ano_escolar} AND 
	  S.MES_PAGO 	= ${v_mes_pago} AND 
	  S.MES_ESCOLAR = ${v_mes_actual}
