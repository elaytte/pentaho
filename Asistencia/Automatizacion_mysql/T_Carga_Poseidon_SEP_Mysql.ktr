  <step>
    <name>Sige_asistencia_declarada_sep</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MySQL_Local_Asistencia_2014_04</connection>
    <sql>select  a.rbd
        ,a.ano_escolar anio_asist
		,a.ano_escolar anio_pago
        ,a.mes_escolar mes_asist        
        ,a.mes_pago
        ,a.cod_ens_subv  cod_ense
        ,a.cod_gra_subv  nivel
        ,sum(a.asis_promedio)       asistencia_mes
        ,sum(a.matricula_inicial)   matricula_inicial
        ,sum(a.altas)               altas
        ,sum(a.bajas)               bajas
        ,sum(a.matricula_final)     matricula_final
from
(select a.ano_escolar,a.mes_pago,a.mes_escolar,a.rbd,a.cod_ens_subv,a.cod_gra_subv,sum(asis_promedio)asis_promedio
,coalesce((select sum(c.matricula) from view_matricula_inicial_sep c where c.ano_escolar = a.ano_escolar and c.mes_escolar = a.mes_escolar and c.rbd = a.rbd and c.cod_ens = a.cod_ens and c.cod_gra = a.cod_gra and c.let_cur = a.let_cur),0) matricula_inicial
,coalesce((select sum(c.matricula) from view_matricula_altas_sep  c where c.ano_escolar = a.ano_escolar and c.mes_escolar = a.mes_escolar and c.rbd = a.rbd and c.cod_ens = a.cod_ens and c.cod_gra = a.cod_gra and c.let_cur = a.let_cur),0) altas
,coalesce((select sum(c.matricula) from view_matricula_bajas_sep  c where c.ano_escolar = a.ano_escolar and c.mes_escolar = a.mes_escolar and c.rbd = a.rbd and c.cod_ens = a.cod_ens and c.cod_gra = a.cod_gra and c.let_cur = a.let_cur),0) bajas
,coalesce((select sum(c.matricula) from view_matricula_final_sep  c where c.ano_escolar = a.ano_escolar and c.mes_escolar = a.mes_escolar and c.rbd = a.rbd and c.cod_ens = a.cod_ens and c.cod_gra = a.cod_gra and c.let_cur = a.let_cur),0) matricula_final
from sige_asistencia_declarada_sep a
where
a.ano_escolar  = ${v_ano_escolar}
and a.mes_pago = ${v_mes_pago}
