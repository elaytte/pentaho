    <sql>select a.rbd,a.ano_escolar anio_asist,a.mes_escolar mes_asist,a.mes_pago,a.cod_ens_subv cod_ense,a.cod_gra_subv nivel,count&#x28;&#x2a;&#x29; matricula&#xd;&#xa;from sige_asistencia_declarada a , sige_asistencia_declarada_pie b, sige_matricula c&#xd;&#xa;where a.ano_escolar &#x3d; b.ano_escolar and&#xd;&#xa;    a.mes_pago &#x3d; b.mes_pago and&#xd;&#xa;    a.mes_escolar &#x3d; b.mes_escolar and&#xd;&#xa;    a.rbd &#x3d; b.rbd and&#xd;&#xa;    a.cod_ens &#x3d; b.cod_ens and&#xd;&#xa;    a.cod_gra &#x3d; b.cod_gra and&#xd;&#xa;    a.let_cur &#x3d; b.let_cur and&#xd;&#xa;    a.sal_run &#x3d; b.sal_run and&#xd;&#xa;    &#xd;&#xa;    b.ano_escolar &#x3d; c.ano_escolar and&#xd;&#xa;    b.rbd &#x3d; c.rbd and&#xd;&#xa;    b.cod_ens &#x3d; c.cod_ens and&#xd;&#xa;    b.cod_gra &#x3d; c.cod_gra and&#xd;&#xa;    b.let_cur &#x3d; c.let_cur and&#xd;&#xa;    b.sal_run &#x3d; c.sal_run and&#xd;&#xa;&#xd;&#xa;    &#x28;month&#x28;c.fecha_incorporacion&#x29; &#x3d; &#x24;&#x7b;v_mes_actual&#x7d;&#x29; and&#xd;&#xa;	a.mes_escolar &#x3d; &#x24;&#x7b;v_mes_actual&#x7d;&#xd;&#xa;group by a.rbd,a.ano_escolar,a.mes_escolar,a.mes_pago,a.cod_ens_subv,a.cod_gra_subv</sql>
