    <connection>Sige_desa</connection>
    <sql>SELECT SA.RBD,SA.ANO_ESCOLAR,SA.MES_ESCOLAR,SA.ANO_ESCOLAR,SA.MES_PAGO,280 COD_ENS, CASE WHEN SA.JECD=0 THEN 1 WHEN SA.JECD = 1 THEN 2 END NIVEL, SUM(SA.ASIS_PROMEDIO) ASISTENCIA_MES
,COALESCE((SELECT VMI.MATRICULA FROM DB2ADMIN.VIEW_MATRICULA_INICIAL_MULTIDIFICIT VMI WHERE SA.MES_ESCOLAR = VMI.MES_ESCOLAR AND SA.ANO_ESCOLAR = VMI.ANO_ESCOLAR AND SA.RBD = VMI.RBD AND (CASE WHEN SA.JECD=0 THEN 1 WHEN SA.JECD = 1 THEN 2 END)=VMI.NIVEL),0) MAT_INICIAL
,COALESCE((SELECT VMB.MATRICULA FROM DB2ADMIN.VIEW_MATRICULA_ALTAS_MULTIDIFICIT VMB WHERE SA.MES_ESCOLAR = VMB.MES_ESCOLAR AND SA.ANO_ESCOLAR = VMB.ANO_ESCOLAR AND SA.RBD = VMB.RBD AND (CASE WHEN SA.JECD=0 THEN 1 WHEN SA.JECD = 1 THEN 2 END)=VMB.NIVEL),0) MAT_ALTAS
,COALESCE((SELECT VMA.MATRICULA FROM DB2ADMIN.VIEW_MATRICULA_BAJAS_MULTIDIFICIT VMA WHERE SA.MES_ESCOLAR = VMA.MES_ESCOLAR AND SA.ANO_ESCOLAR = VMA.ANO_ESCOLAR AND SA.RBD = VMA.RBD AND (CASE WHEN SA.JECD=0 THEN 1 WHEN SA.JECD = 1 THEN 2 END)=VMA.NIVEL),0) MAT_BAJA
,COALESCE((SELECT VMF.MATRICULA FROM DB2ADMIN.VIEW_MATRICULA_FINAL_MULTIDIFICIT VMF WHERE SA.MES_ESCOLAR = VMF.MES_ESCOLAR AND SA.ANO_ESCOLAR = VMF.ANO_ESCOLAR AND SA.RBD = VMF.RBD AND (CASE WHEN SA.JECD=0 THEN 1 WHEN SA.JECD = 1 THEN 2 END)=VMF.NIVEL),0) MAT_FINAL
FROM DB2ADMIN.SIGE_ASISTENCIA_DECLARADA_MULTIDEFICIT SA
WHERE SA.MES_PAGO = 12
and SA.MES_ESCOLAR = 11
and SA.ANO_ESCOLAR = 2012
AND SA.RBD IN (SELECT DISTINCT RBD FROM DB2ADMIN.SIGE_MATRICULA  WHERE ANO_ESCOLAR = 2012 AND COD_ENS IN (211,212,213,214,215,216,217))
AND SA.COD_ENS IN (211,212,213,214,215,216,217)
and (SA.RBD,SA.SAL_RUN) IN (SELECT RBD,SAL_RUN FROM DB2ADMIN.SIGE_RBD_RUN_MULTIDEFICIT WHERE ANO_ESCOLAR = 2012)
GROUP BY SA.RBD,SA.ANO_ESCOLAR,SA.MES_ESCOLAR,SA.ANO_ESCOLAR,SA.MES_PAGO,SA.JECD
