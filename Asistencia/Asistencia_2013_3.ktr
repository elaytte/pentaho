    <connection>Sige_desa</connection>
    <sql>SELECT A.ANO_ESCOLAR,A.ANO_ESCOLAR,A.MES_PAGO,A.MES_ESCOLAR,A.RBD,A.COD_ENS_SUBV,A.COD_GRA_SUBV,
COALESCE(SUM(A.ASIS_PROMEDIO),0) ASIS_PROMEDIO,
COALESCE(SUM(A.MATRICULA_INICIAL),0) MATRICULA_INICIAL,
COALESCE(SUM(A.ALTAS),0) ALTAS,
COALESCE(SUM(A.BAJAS),0) BAJAS,
COALESCE(SUM(A.MATRICULA_FINAL),0) MATRICULA_FINAL
FROM
(SELECT A.ANO_ESCOLAR,A.MES_PAGO,A.MES_ESCOLAR,A.RBD,A.COD_ENS_SUBV,A.COD_GRA_SUBV,
COALESCE(SUM(ASIS_PROMEDIO),0) ASIS_PROMEDIO
,COALESCE((SELECT SUM(C.MATRICULA) FROM DB2ADMIN.VIEW_MATRICULA_INICIAL_PIE C WHERE C.ANO_ESCOLAR = 2012 AND C.MES_ESCOLAR = A.MES_ESCOLAR AND C.RBD = A.RBD AND C.COD_ENS = A.COD_ENS AND C.COD_GRA = A.COD_GRA AND C.LET_CUR = A.LET_CUR),0) MATRICULA_INICIAL
,COALESCE((SELECT SUM(C.MATRICULA) FROM DB2ADMIN.VIEW_MATRICULA_ALTAS_PIE  C WHERE C.ANO_ESCOLAR = 2012 AND C.MES_ESCOLAR = A.MES_ESCOLAR AND C.RBD = A.RBD AND C.COD_ENS = A.COD_ENS AND C.COD_GRA = A.COD_GRA AND C.LET_CUR = A.LET_CUR),0) ALTAS
,COALESCE((SELECT SUM(C.MATRICULA) FROM DB2ADMIN.VIEW_MATRICULA_BAJAS_PIE  C WHERE C.ANO_ESCOLAR = 2012 AND C.MES_ESCOLAR = A.MES_ESCOLAR AND C.RBD = A.RBD AND C.COD_ENS = A.COD_ENS AND C.COD_GRA = A.COD_GRA AND C.LET_CUR = A.LET_CUR),0) BAJAS
,COALESCE((SELECT SUM(C.MATRICULA) FROM DB2ADMIN.VIEW_MATRICULA_FINAL_PIE  C WHERE C.ANO_ESCOLAR = 2012 AND C.MES_ESCOLAR = A.MES_ESCOLAR AND C.RBD = A.RBD AND C.COD_ENS = A.COD_ENS AND C.COD_GRA = A.COD_GRA AND C.LET_CUR = A.LET_CUR),0) MATRICULA_FINAL
FROM DB2ADMIN.SIGE_ASISTENCIA_DECLARADA_PIE A
WHERE
A.ANO_ESCOLAR=2012 
AND A.MES_PAGO=12 
AND A.MES_ESCOLAR=11 
AND A.ESTADO_PAGO=1 AND A.RBD&gt;0  AND RBD&lt;60000
AND A.COD_ENS_SUBV IS NOT NULL AND  A.COD_GRA_SUBV IS NOT NULL
GROUP BY  A.ANO_ESCOLAR,A.MES_PAGO,A.MES_ESCOLAR,A.RBD,A.COD_ENS_SUBV,A.COD_GRA_SUBV,A.COD_ENS,A.COD_GRA, A.LET_CUR
WITH UR
) A
GROUP BY A.ANO_ESCOLAR,A.MES_PAGO,A.MES_ESCOLAR,A.RBD,A.COD_ENS_SUBV,A.COD_GRA_SUBV</sql>
