    <connection>SIGE_PROD</connection>
    <sql>SELECT distinct A.SAL_RUN, SAL_DGV, SAL_APP
,CASE WHEN LENGTH(TRIM(SAL_APM))=0 THEN '-' ELSE SAL_APM END SAL_APM
,SAL_NOM, SAL_GENERO, SAL_FEC_NAC, ESTADO_CIVIL
,CASE WHEN LENGTH(TRIM(SAL_MAIL))=0 THEN '-' ELSE SAL_MAIL END SAL_MAIL
,CASE WHEN LENGTH(TRIM(SAL_DIR))=0 THEN '-' ELSE SAL_DIR END SAL_DIR
,SAL_FEC_DEFUN, SAL_NACIONALIDAD, SAL_FEC_MATR, IND_VALREGCIVIL
,CASE WHEN LENGTH(TRIM(SAL_FONO))=0 THEN '-' ELSE SAL_FONO END SAL_FONO
,CASE WHEN LENGTH(TRIM(SAL_CELULAR))=0 THEN '-' ELSE SAL_CELULAR END SAL_CELULAR
,FEC_ULTIMA_ACT, HRA_ULTIMA_ACT, SAL_COD_COMUNA, SAL_COD_REGION, SAL_ESTADO, COD_ETNIA, SAL_EMBARAZO, A.TIPO_ALUMNO, SOLICITA_VALIDACION
,CASE WHEN LENGTH(TRIM(SAL_DIR_NUMERO))=0 THEN '-' ELSE SAL_DIR_NUMERO END SAL_DIR_NUMERO
,CASE WHEN LENGTH(TRIM(SAL_DIR_NUM_DEPTO_CASA))=0 THEN '-' ELSE SAL_DIR_NUM_DEPTO_CASA END SAL_DIR_NUM_DEPTO_CASA
,CASE WHEN LENGTH(TRIM(SAL_DIR_INFO_ADICIONAL))=0 THEN '-' ELSE SAL_DIR_INFO_ADICIONAL END SAL_DIR_INFO_ADICIONAL
,SAL_PAIS_ORIGEN 
 FROM DB2ADMIN.SIGE_FICHA_ALUMNO A, DB2ADMIN.SIGE_MATRICULA B 
WHERE A.SAL_RUN=B.SAL_RUN AND B.ANO_ESCOLAR =  ${ANIO}
AND B.RBD in (SELECT RBD FROM DB2ADMIN.TMP_RBDS_BORRAR)


</sql>
