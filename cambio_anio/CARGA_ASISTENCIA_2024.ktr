    <sql>SELECT DT.MES_ESCOLAR, SM.RBD,SM.COD_ENS,SM.COD_GRA,trim(SM.LET_CUR) LET_CUR, 2025 ANO_ESCOLAR,trim(SM.SAL_RUN) SAL_RUN,DT.DIA1,DT.DIA2,DT.DIA3,DT.DIA4,DT.DIA5,DT.DIA6,DT.DIA7,DT.DIA8,DT.DIA9,DT.DIA10,DT.DIA11,DT.DIA12,DT.DIA13,DT.DIA14,DT.DIA15,DT.DIA16,DT.DIA17,DT.DIA18,DT.DIA19,DT.DIA20,DT.DIA21,DT.DIA22,DT.DIA23,DT.DIA24,DT.DIA25,DT.DIA26,DT.DIA27,DT.DIA28,DT.DIA29,DT.DIA30,DT.DIA31,DT.DIAS_TRABAJADOS, 0 AS ASIS_PROMEDIO,0 AS ESTADO_DECLARACION,0 AS DIAS_ASISTIDOS,            'adm' AS USER_INTERNO,         '0.0.0.0' AS IP_INTERNO, '1900-01-01' AS FECHA_ACTUALIZACION,  CURRENT_TIME AS HORA_ACTUALIZACION,      0 AS CUR_AUT_ESTADO
FROM  DB2ADMIN.SIGE_MATRICULA SM INNER JOIN DB2ADMIN.SIGE_DIAS_TRABAJAR DT 
                        ON SM.RBD = DT.RBD 
                        AND SM.COD_ENS = DT.COD_ENS
                        AND SM.COD_GRA = DT.COD_GRA
                        AND SM.LET_CUR = DT.LET_CUR
                        AND DT.MES_ESCOLAR = 3
