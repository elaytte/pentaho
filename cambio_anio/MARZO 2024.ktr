    <sql>SELECT A.ANO_ESCOLAR,A.RBD,A.COD_ENS,A.COD_GRA,A.LET_CUR,a.SAL_RUN,B.FECHA_INCORPORACION VALOR_REAL
FROM DB2ADMIN.SIGE_MATRICULA A, DB2ADMIN.SIGE_MATRICULA_AJUSTE B
WHERE A.ANO_ESCOLAR = 2024
      AND A.ANO_ESCOLAR = B.ANO_ESCOLAR
      AND A.RBD = B.RBD_PRE
      AND A.COD_ENS = B.COD_ENS_PRE
      AND A.COD_GRA = B.COD_GRA_PRE 
      AND A.LET_CUR = B.LET_CUR_PRE
      AND A.SAL_RUN = B.SAL_RUN
      AND YEAR(A.FECHA_INCORPORACION)=2024
      AND MONTH(A.FECHA_INCORPORACION)=3
      AND DAY(A.FECHA_INCORPORACION) = 1      
      AND YEAR(B.FECHA_INCORPORACION)=2024
      AND MONTH(B.FECHA_INCORPORACION)=3
      AND DAY(B.FECHA_INCORPORACION) > 1      
WITH UR
--AND A.FECHA_ULTIMA_ACTUALIZACION = '2024-03-11'
