    <name>SIGE_MATRICULA_AJUSTE</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>SIGELAB</connection>
    <sql>SELECT ANO_ESCOLAR, trim(SAL_RUN) SAL_RUN, RBD_PRE RBD, COD_ENS_PRE COD_ENS, COD_GRA_PRE COD_GRA, trim(LET_CUR_PRE) LET_CUR, '0' ESTADO_COLISION , 0 CONFIRMA_ASISTENCIA, 0 ALUMNO_EXEDENTE, 2 TIPO_ALUMNO, 0 ESTADO_MATRICULA
,'2024-03-01'  FECHA_INCORPORACION, '1900-01-01' FECHA_RETIRO, 0 SAL_REPITE_GRADO, 0 SAL_COD_PIE, 0 SAL_DIFERENCIAL, 0 COD_SEC, 0 COD_ESP, 0 ORDEN_MATRICULA, 0 PROMEDIO_FINAL, 0 PORCEN_ASISTENCIA, ' ' OBS_PROMOCION, 0 REPRUEBA_ASISTENCIA
, 0 SITUACION_FINAL, CURRENT DATE FECHA_ULTIMA_ACTUALIZACION, CURRENT TIMESTAMP FECHA_INGRESO_SISTEMA, '1900-01-01 00:00:00' FECHA_RETIRO_SISTEMA, 0 PIE_FUENTE_FINAN, 0 PIE_NRES, '1900-01-01' PIE_FECHA_RES,'-' USER_INTERNO, '0.0.0.0' IP_INTERNO
, CURRENT TIME HORA_ACTUALIZACION, IND_VULNERABLE
FROM db2admin.SIGE_MATRICULA_AJUSTE
WHERE ANO_ESCOLAR = 2024

and RBD_PRE in(SELECT RBD FROM db2admin.SIGE_ESTABLECIMIENTO WHERE ESTADO_ESTAB = 1)
and rbd_pre >= 0 and rbd_pre &lt;2
--and rbd_pre >= 0 and rbd_pre &lt;60000
--and rbd_pre >= 10000 and rbd_pre &lt;60000  and rbd_pre = 2206

--and (ANO_ESCOLAR,RBD_PRE,COD_ENS_PRE,COD_GRA_PRE,LET_CUR_PRE) IN (SELECT ANO_ESCOLAR,rbd,COD_ENS,COD_GRA,LET_CUR FROM db2admin.SIGE_ESTRUCTURA_CURSO WHERE ANO_ESCOLAR = 2024)
</sql>
