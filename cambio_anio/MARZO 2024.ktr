    <sql>SELECT 2024 ANO_ESCOLAR,SDT.MES_ESCOLAR,SEC.RBD,SEC.COD_ENS,SEC.COD_GRA,SEC.LET_CUR,  DIA1, DIA2, DIA3, DIA4, DIA5, DIA6, DIA7, DIA8, DIA9, DIA10, DIA11, DIA12, DIA13, DIA14, DIA15, DIA16, DIA17, DIA18, DIA19, DIA20, DIA21, DIA22, DIA23, DIA24, DIA25, DIA26, DIA27, DIA28, DIA29, DIA30, DIA31, SDT.FECHA_ACTUALIZACION, SDT.HORA_ACTUALIZACION, SDT.DIAS_TRABAJADOS, 0 ESTADO_DECLARACION, 1 AUT_DEPROV
FROM DB2ADMIN.SIGE_ESTRUCTURA_CURSO SEC, DB2ADMIN.SIGE_ESTABLECIMIENTO SE, DB2ADMIN.SIGE_DIAS_TRABAJAR_OFICIAL SDT
WHERE SEC.ANO_ESCOLAR = 2024
AND SDT.MES_ESCOLAR in (3)
AND SEC.RBD = SE.RBD
AND SEC.RBD IN(
				SELECT RBD FROM DB2ADMIN.SIGE_ESTABLECIMIENTO 
                WHERE RBD >= 1
                AND RBD &lt;= 60000
                )
AND SE.COD_REGION = SDT.REGION
AND SE.CP_DEPROV = SDT.DEPROV
AND SDT.ANO_ESCOLAR = SEC.ANO_ESCOLAR
AND (SEC.COD_ENS,SEC.COD_GRA,SEC.ANO_ESCOLAR) IN (select COD_ENSENANZA,COD_GRADO,ANO_ESCOLAR from DB2ADMIN.SIGE_ENSENANZA_GRADO  where ANO_ESCOLAR = 2024 AND ASISTENCIA > 0)
ORDER BY SEC.RBD,SEC.COD_ENS,SEC.COD_GRA,SEC.LET_CUR ASC
